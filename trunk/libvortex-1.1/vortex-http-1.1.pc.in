prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: libvortex-tls
Description: Robust, full featured BEEP implementation (RFC 3080/RFC 3081)
  HTTP CONNECT support for Vortex 1.1. A component that allows BEEP 
  connection through an HTTP/1.1 proxy.
Requires: vortex-1.1
Version: @VORTEX_VERSION@
Libs: -L${libdir} 
Cflags: -I${includedir}/vortex-1.1
